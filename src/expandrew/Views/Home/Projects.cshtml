@model IEnumerable<expandrew.Models.Project>

<h2>All Projects</h2>
<hr />
<ol>
    @foreach (var project in Model)
    {
        <li>
            <ul>
                <li>Name: @Html.DisplayFor(modelItem => project.name)</li>
                <li>Description: @Html.DisplayFor(modelItem => project.description)</li>
                <li>URL: @Html.DisplayFor(modelItem => project.url)</li>
                <li>Stars: @Html.DisplayFor(modelItem => project.stargazers_count)</li>
            </ul>
        </li>
    }
</ol>

@Html.ActionLink("Back home", "Index")